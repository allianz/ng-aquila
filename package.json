{
  "name": "ng-aquila",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "cross-env NODE_OPTIONS=--max-old-space-size=7680 ng",
    "release": "standard-version -n",
    "start": "npm run build:package && npm run library:generate:docs && ng serve opensource-documentation --port=4200",
    "build": "ng build",
    "test": "ng test",
    "lint:ts": "npm run build:tslint-rules && tslint -c projects/ng-aquila/tslint.json -p projects/ng-aquila/tsconfig.lib.json -e **/node_modules/**",
    "lint:scss": "sass-lint -c .sass-lint.yml -q -v",
    "lint": "npm-run-all lint:ts lint:scss",
    "lint:commitmsg": "commitlint -e",
    "lint-staged": "lint-staged --relative",
    "build:all:ivy": "npm-run-all build:lib:ivy build:docs",
    "build:all:ve": "npm-run-all build:lib build:docs",
    "build:lib": "npm run ng-high-memory -- build ng-aquila --configuration production && npm run build:package",
    "build:lib:ivy": "npm run ng-high-memory -- build ng-aquila && npm run build:package",
    "build:docs": "npm run ng-high-memory -- build opensource-documentation --configuration production",
    "build:tslint-rules": "tsc -p tools/tslint-rules/tsconfig.json",
    "postinstall": "node projects/ng-aquila/src/phone-input/extract-calling-codes.js",
    "test:lib": "npm run build:package && npm run test -- ng-aquila --configuration viewengine",
    "test:lib:ivy": "npm run build:package && npm run test -- ng-aquila",
    "test:schematics": "npm run build:package && npm run test:schematics:jasmine",
    "test:schematics:jasmine": "jasmine --config=projects/ng-aquila/src/schematics/test/jasmine.json",
    "build:package": "node ./scripts/build-package.js",
    "library:generate:docs": "npm run cli -- generate projects/ng-aquila",
    "cli": "ts-node --project projects/ngx-docs-cli/tsconfig.json ./projects/ngx-docs-cli/src/main"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/aposin/ng-aquila.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint:commitmsg",
      "pre-commit": "npm run build:tslint-rules && npm run lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.10",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "^12.2.10",
    "@angular/compiler": "^12.2.10",
    "@angular/core": "^12.2.10",
    "@angular/forms": "^12.2.10",
    "@angular/platform-browser": "^12.2.10",
    "@angular/platform-browser-dynamic": "^12.2.10",
    "@angular/router": "^12.2.10",
    "@schematics/angular": "^12.2.10",
    "chalk": "^4.1.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.18.3",
    "css-vars-ponyfill": "^2.4.7",
    "dayjs": "^1.10.7",
    "decimal.js": "^10.3.1",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.28.4",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "highlight.js": "^10.7.3",
    "i18n-iso-countries": "^6.8.0",
    "iban": "^0.0.14",
    "libphonenumber-js": "^1.9.38",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^6.0.1",
    "moment": "^2.29.1",
    "ngx-color-picker": "^11.0.0",
    "parse-color": "^1.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.10",
    "@angular-devkit/core": "^12.2.10",
    "@angular-devkit/schematics": "^12.2.10",
    "@angular-devkit/schematics-cli": "^0.1101.4",
    "@angular/cli": "^12.2.10",
    "@angular/compiler-cli": "^12.2.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@types/fs-extra": "9.0.6",
    "@types/iban": "0.0.32",
    "@types/jasmine": "^3.10.0",
    "@types/node": "^12.20.33",
    "@types/parse-color": "^1.0.1",
    "@types/shelljs": "^0.8.9",
    "axe-core": "^4.3.3",
    "codelyzer": "^6.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "husky": "^4.3.8",
    "jasmine": "^3.10.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-viewport": "^1.0.8",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^12.2.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.43.2",
    "sass-lint": "^1.13.1",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.2",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "tslint-jasmine-rules": "^1.6.1",
    "typescript": "4.2.4"
  }
}
