{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "aquila-docs-example": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [],
            "styles": [
              "node_modules/@angular/cdk/a11y-prebuilt.css",
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "src/styles.scss",
              "node_modules/@allianz/ng-aquila/css/normalize.css",
              "node_modules/@allianz/ng-aquila/css/utilities.css",
              "node_modules/@allianz/ng-aquila/themes/aquila.css"
            ],
            "scripts": [],
            "outputHashing": "all"
          }
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "buildTarget": "aquila-docs-example:build"
          }
        }
      }
    }
  }
}
